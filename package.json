    "url": "https://github.com/ningsk/card-sdk/issues"
  },
  "homepage": "https://github.com/ningsk/card-sdk#readme",
  "dependencies": {
    "cesium": "1.73.0",
    "echarts": "^4.8.0",
    "jquery": "^3.5.1",
    "leaflet": "^1.6.0",
    "turf": "^3.0.14"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^6.0.3",
    "css-loader": "^3.2.0",
    "esri-leaflet": "^2.5.0",
    "html-webpack-plugin": "^3.2.0",
    "strip-pragma-loader": "^1.0.0",
