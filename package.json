  "module": "dist/mars3d-src.esm.js",
  "unpkg": "dist/mars3d-src.js",
  "scripts": {
    "pretest": "yarn run lint",
    "test": "yarn run test-nolint",
    "test-nolint": "karma start ./spec/karma.conf.js",
    "build": "yarn run rollup",
    "lint": "eslint src spec/suites docs/docs/js",
    "lintfix": "yarn run lint -- --fix",
    "rollup": "rollup -c build/rollup-config.js",
    "watch": "rollup -w -c build/rollup-watch-config.js",
    "integrity": "node ./build/integrity.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ningsk/card-sdk.git"
  },
  "author": "",
